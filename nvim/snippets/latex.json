  "table": {
    "prefix": "table",
    "description": "Table environment",
    "body": [
      "\\begin{table}[${1:htpb}]",
      "\t\\centering",
      "\t\\caption{${2:caption}}",
      "\t\\label{tab:${3:label}}",
      "\t\\begin{tabular}{${5:c}}",
      "\t$0${5/((?<=.)c|l|r)|./(?1: & )/g}",
      "\t\\end{tabular}",
      "\\end{table}"
    ]
  },
