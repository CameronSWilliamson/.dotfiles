  "mk": {
    "prefix": "mk",
    "description": "Math",
    "body": [
      "$${1}$`!p",
      "if t[2] and t[2][0] not in [',', '.', '?', '-', ' ']:",
      "\tsnip.rv = ' '",
      "else:",
      "\tsnip.rv = ''",
      "`$2"
